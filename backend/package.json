    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.14",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.5.0",
    "resend": "^4.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
